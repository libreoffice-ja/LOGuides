      </tuv>
    </tu>
    <tu>
      <tuv lang="en-US">
        <seg>Flowchart</seg>
      </tuv>
      <tuv lang="ja" changeid="jun" changedate="20210417T113149Z" creationid="jun" creationdate="20210417T113149Z">
        <seg>フローチャート</seg>
      </tuv>
